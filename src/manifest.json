{
	"name": "__MSG_extensionName__",
	"description": "__MSG_extensionDescription__",
	"version": "1.1.0",
	"developer": {
		"name": "Aldo Barreras",
		"url": "https://github.com/aldobarr/hide_private_mode"
	},
	"options_ui": {
		"page": "options.html"
	},
	"icons": {
		"48": "icon.svg",
		"96": "icon.svg"
	},
	"content_scripts": [
		{
			"all_frames": true,
			"js": [ "contentLoader.ts" ],
			"matches": [ "<all_urls>" ],
			"run_at": "document_start"
		}
	],
	"default_locale": "en",
	"manifest_version": 2,
	"permissions": [
		"<all_urls>"
	],
	"web_accessible_resources": [
		"content.ts"
	]
}
